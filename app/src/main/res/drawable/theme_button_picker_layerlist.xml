<layer-list    xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item android:height="@dimen/menu_height" android:gravity="center">
        <shape
            android:shape="rectangle">
            <solid
                android:color="?themeColorPrimary"
                ></solid>
        </shape>
    </item>
    <item android:height="@dimen/button_height" android:gravity="center" android:left="@dimen/card_width" android:right="@dimen/card_width">
        <shape
            android:shape="rectangle">
            <corners android:radius="@dimen/buttonRadius" />
            <stroke
                android:color="?themeButtonStroke"
                android:width="@dimen/buttonStroke"
                >

            </stroke>


            <solid
                android:color="?themeButtonSolid"
                ></solid>
        </shape>
    </item>

</layer-list>
